raw_data = load '$INPUT_FILES' using com.twitter.elephantbird.examples.proto.pig.load.LzoAddressBookProtobufBlockPigLoader()
      as (
        person: bag {
          person_tuple: tuple (
              name: chararray,
              id: int,
              email: chararray,
              phone: bag {
                phone_tuple: tuple (
                    number: chararray,
                    type: chararray
                )
              }
          )
        }
      );



